#!/usr/bin/env python3

import sys

from redun.cli import RedunClient, RedunClientError

if __name__ == "__main__":
    client = RedunClient()
    try:
        client.execute()
    except RedunClientError as error:
        print(
            "{error_type}: {error}".format(
                error_type=type(error).__name__,
                error=str(error),
            )
        )
        sys.exit(1)
